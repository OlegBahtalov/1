import math


def calculate_function(x, y, function_type):
    if function_type == 1:
        return math.sinh(y)
    elif function_type == 2:
        return x ** math.exp(x)
    elif function_type == 3:
        return math.sqrt(math.log(x))
    else:
        print('Ошибка: неверный выбор ')
        return None


def compute_result(x, y, fx):
    product = abs(x * y)

    if product > 10:
        return math.log(abs(fx) + abs(y))
    elif product < 10:
        return math.exp(fx + y)
    elif product == 10:
        return (fx - y) ** (1 / 3)
    else:
        return 'No music'


def main():
    x_value = float(input('Введите значение x: '))
    y_value = float(input('Введите значение y: '))
    function_choice = float(input('Выберите тип функции (1 -> sinh(y), 2 -> x^(e^x), 3 -> sqrt(ln(x)): '))

    fx_value = calculate_function(x_value, y_value, function_choice)

    if fx_value is not None:
        result = compute_result(x_value, y_value, fx_value)
        print('Результат: ', result)


if __name__ == '__main__':
    main()
    input('Тыкаем enter и все вырубаем ')
